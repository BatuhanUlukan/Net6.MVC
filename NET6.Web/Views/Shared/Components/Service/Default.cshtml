@using NET6.Entity.DTOs.Processes;
@model ProcessListDto

@foreach (var process in Model.Processes)
{

    <div class="col-sm-6 col-lg-3">

        <!-- service -->
        <div class="service">
            <img src="images/@process.Image.FileName" alt="image" />
            <h3>@process.Title</h3>
            <p>
                @Html.Raw(@process.Content)
            </p>
        </div>
        <!-- service -->
    </div>
}
